<view class="bg-white page"><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">添加车辆</view></cu-custom><view><swiper class="{{['card-swiper',dotStyle?'square-dot':'round-dot']}}" indicator-dots="{{true}}" circular="{{true}}" current="{{cardCur}}" autoplay="{{false}}" indicator-color="#8799a3" indicator-active-color="#0081ff" data-event-opts="{{[['change',[['cardSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="{{[cardCur==index?'cur':'']}}"><view class="swiper-item" style="position:relative;"><image src="{{item.url}}" mode="aspectFill"></image><view class="car-item-contet"><text>{{item.title}}</text></view></view></swiper-item></block></swiper></view><view class="bg-gray padding-sm"></view><view class="add-car-detail bg-white"><view class="detail-item-tile"><text class="detail-item-title-font">姓名</text></view><view class="detail-item-content"><input class="detail-item-input bg-gray" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['','userName','$event',[]]]]]]}}" value="{{userName}}" bindinput="__e"/></view><view class="detail-item-tile"><text class="detail-item-title-font">联系手机</text></view><view class="detail-item-content"><input class="detail-item-input bg-gray" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','phoneNumber','$event',[]]]]]]}}" value="{{phoneNumber}}" bindinput="__e"/></view><view class="detail-item-tile"><text class="detail-item-title-font">车牌号码</text><view data-event-opts="{{[['tap',[['doScanCarNum',['$event']]]]]}}" style="display:flex;align-items:center;margin-left:auto;margin-right:20px;" bindtap="__e"><image style="width:20px;height:20px;margin-right:5rpx;" src="/static/scan.png" mode="aspectFit"></image><text>拍照识别车牌号</text></view></view><view class="detail-item-content"><view class="carinput-input"><block wx:for="{{carInput}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view hidden="{{!(i<7)}}" class="{{['carinput-input-i',(carIndex==i)?'input-active':'']}}" data-index="{{i}}" data-event-opts="{{[['tap',[['inputKey',['$event']]]]]}}" bindtap="__e">{{''+carInput[i].val+''}}</view></block><view class="{{['carinput-input-i',(carIndex==7)?'input-active':'']}}" data-index="7" data-event-opts="{{[['tap',[['inputKey',['$event']]]]]}}" bindtap="__e">{{''+(carInput[7].val?carInput[7].val:'+')+''}}</view></view><view class="carinput-power"><checkbox-group data-event-opts="{{[['change',[['powerChange',['$event']]]]]}}" class="checkbox-g" bindchange="__e"><label class="checkbox"><checkbox value="true" checked="{{isPower}}"></checkbox>新能源</label></checkbox-group></view></view></view><view class="detail-bottom"><button class="save-button-addCar" type="primary" size="default" data-event-opts="{{[['tap',[['toBind',['$event']]]]]}}" bindtap="__e">{{addType==3?'保存':'确认添加'}}</button></view><tki-float-keyboard class="vue-ref" vue-id="2" mode="car" type="{{keyType}}" title="车牌键盘" data-ref="keybd" data-event-opts="{{[['^del',[['keyCbDel']]],['^val',[['keyCbVal']]],['^hide',[['keyCbHide']]]]}}" bind:del="__e" bind:val="__e" bind:hide="__e" bind:__l="__l"></tki-float-keyboard></view>